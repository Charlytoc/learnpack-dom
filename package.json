{
  "name": "learnpack-dom",
  "description": "Node compiler for DOM learnpack tutorials",
  "version": "0.0.1",
  "author": "Tomas Gonzalez @tommygonzaleza",
  "bugs": "https://github.com/tommygonzaleza/learnpack-dom/issues",
  "dependencies": {
    "@babel/core": "^7.14.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.14.8",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "chalk": "^2.4.2",
    "compile-run": "^2.3.4",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "prettier": "^2.3.2",
    "rewire": "^5.0.0",
    "shelljs": "^0.8.4",
    "source-map-support": "^0.5.19",
    "webpack": "^5.40.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/test": "^1.2.8",
    "chai": "^4.3.4",
    "eslint": "^6.8.0",
    "eslint-config-oclif": "^3.1.0",
    "globby": "^11.0.4",
    "mocha": "^9.0.2",
    "nyc": "^15.1.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/src"
  ],
  "homepage": "https://github.com/learnpack/node-compiler",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "https://learnpack.co/terms",
  "oclif": {
    "commands": "./src/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "hooks": {
      "action": "./src/plugin.js"
    }
  },
  "repository": "tommygonzaleza/learnpack-dom",
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.js\"",
    "version": "oclif-dev readme && git add README.md"
  }
}