    "@testing-library/dom": "^8.10.1",
    "babel-core": "^6.26.3",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "chalk": "^2.4.2",
    "compile-run": "^2.3.4",
    "css-loader": "^6.2.0",
    "jest": "^27.0.6",
    "learnpack": "^0.1.27",
    "mini-css-extract-plugin": "^2.1.0",
