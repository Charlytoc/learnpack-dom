    ],
    "hooks": {
      "action": "./src/plugin.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tommygonzaleza/learnpack-dom.git"
  },
  "scripts": {
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "eslint .",
    "prepack": "oclif-dev manifest && oclif-dev readme",
